{
  "name": "moberries",
  "description": "",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "webpack --watch",
    "build": "yarn clean && yarn babel src -D -d dist/src",
    "start:staging": "NODE_ENV=staging && webpack && node dist",
    "start:prod": "NODE_ENV=production && webpack && node dist",
    "lint:fix": "eslint --fix src",
    "lint": "eslint src",
    "test": "jest --coverage --forceExit --detectOpenHandles",
    "unit-test": "jest --silent --coverage --forceExit --detectOpenHandles --setupFiles dotenv/config",
    "unit-test:watch": "jest --detectOpenHandles --watch --setupFiles dotenv/config",
    "db:model:generate": "sequelize model:generate --name",
    "db:model:auto-generate": "sequelize-auto -h localhost -d api -u api -x api -p 5004 -e postgres --camel -o ./src/database/models ",
    "db:migrate:generate": "sequelize migration:generate --name",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed:generate": "sequelize seed:generate --name",
    "db:seed": "sequelize db:seed --seed",
    "db:seed:undo": "sequelize db:seed:undo --seed",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "db:seed:all": "sequelize db:seed:all"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.0.0",
    "express": "^4.16.4",
    "express-validation": "^1.0.3",
    "express-winston": "^3.1.0",
    "joi": "^14.3.1",
    "lodash": "^4.17.15",
    "luxon": "^1.21.3",
    "pg": "^7.14.0",
    "sequelize": "^5.19.6",
    "swagger-jsdoc": "^3.2.3",
    "swagger-ui-express": "^4.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "eslint": "^6.6.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-jest": "^21.24.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-junit": "^5.2.0",
    "nodemon-webpack-plugin": "^4.0.6",
    "sequelize-cli": "^5.5.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
