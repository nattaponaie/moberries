{
  "presets": [
    "env",
    "stage-0"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "configs": "./src/configs",
          "utils": "./src/utils",
          "models": "./src/database/models",
          "middleware": "./src/api/middleware",
          "api": "./src/api"
        }
      }
    ]
  ]
}